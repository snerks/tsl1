{
    "compilerOptions": {
        "target": "es6",
        "jsx": "react",
        "noImplicitAny": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "noEmitOnError": false,
        "moduleResolution": "classic",
        "listFiles": true
    },
    "exclude": [
        "node_modules"
    ]
}
